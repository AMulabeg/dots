lcmd - j : yabai -m window --focus south
lcmd - k : yabai -m window --focus north
lcmd - h : yabai -m window --focus west
lcmd - l : yabai -m window --focus east
lcmd - s: yabai -m display --focus west
lcmd - g: yabai -m display --focus east

ctrl + shift - j : yabai -m display --focus south
ctrl + shift - k : yabai -m display --focus north
ctrl + shift - l : yabai -m display --focus east
ctrl + shift - h : yabai -m display --focus west
shift + lcmd - r : yabai -m space --rotate 270
# flip along y-axis
shift + lcmd - y : yabai -m space --mirror y-axis

# flip along x-axis
shift + lcmd - x : yabai -m space --mirror x-axis

# toggle window float
shift + rcmd - t : yabai -m window --toggle float --grid 5:5:3:3:4:4 
shift + lcmd - m : yabai -m window --toggle zoom-fullscreen

# balance out tree of windows (resize to occupy same area)
shift + lcmd - e : yabai -m space --balance
shift + lcmd - j : yabai -m window --swap south
shift + lcmd - k : yabai -m window --swap north
shift + lcmd - h : yabai -m window --swap west
shift + lcmd - l : yabai -m window --swap east
# move window and split
ctrl + lcmd - j : yabai -m window --warp south
ctrl + lcmd - k : yabai -m window --warp north
ctrl + lcmd - h : yabai -m window --warp west
ctrl + lcmd - l : yabai -m window --warp east
# stop/start/restart yabai
ctrl + lcmd - q : brew services stop yabai
ctrl + lcmd - s : brew services start yabai
ctrl + lcmd - r : brew services restart yabai
shift + lcmd - p : yabai -m window --space prev;
shift + lcmd - n : yabai -m window --space next;
shift + lcmd - 1 : yabai -m window --space 1;
shift + lcmd - 2 : yabai -m window --space 2;
shift + lcmd - 3 : yabai -m window --space 3;
shift + lcmd - 4 : yabai -m window --space 4;
shift + lcmd - 5 : yabai -m window --space 5;
shift + lcmd - 6 : yabai -m window --space 6;
shift + lcmd - 7 : yabai -m window --space 7;


